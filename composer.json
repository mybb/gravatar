{
	"name": "mybb/gravatar",
	"description": "Easy Gravatar generation for Laravel 5.0.",
	"license": "BSD-3",
	"authors": [
		{
			"name": "Euan T",
			"email": "euantor@mybb.com"
		}
	],
	"autoload": {
		"psr-4": {
			"MyBB\\Gravatar\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"MyBB\\Gravatar\\Test\\": "tests/"
		}
	},
	"require": {
		"php": ">=7.0.0",
		"illuminate/support": "^5.3"
	},
	"require-dev": {
		"phpunit/phpunit": "5.5.*",
		"squizlabs/php_codesniffer": "~2.0@dev"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"vendor-dir": "vendor",
		"preferred-install": "dist"
	}
}
